{
  "name": "react-with-flask",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently -k -s first \"bash -c 'cd Backend/db && docker-compose up -d'\" \"bash -c 'cd Backend && ../venv/bin/python -m flask --app app run --no-debugger'\" \"npm run dev\"",
    "dev:win": "concurrently \"cd Backend && ..\\venv\\Scripts\\python.exe -m flask --app app run --no-debugger\" \"npm run dev\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "api": "cd api && python -m flask --app api run --no-debugger"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "express": "^5.1.0",
    "flask": "^0.2.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "styled-components": "^6.1.19",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
